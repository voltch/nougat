BB=/system/xbin/busybox;

if [ "$($BB mount | grep rootfs | cut -c 26-27 | grep -c ro)" -eq "1" ]; then
	$BB mount -o remount,rw /;
fi;

P2=/res/synapse/volt/hotplug;


if [[ ! -z $2 ]]; then
	echo "$2" > "$1"
fi

$BB echo "$(cat "$1")"
FREQ=$(cat /res/synapse/volt/hotplug);
$BB echo $FREQ > /sys/power/cpuhotplug/enabled;

$BB mount -t rootfs -o remount,ro rootfs;



